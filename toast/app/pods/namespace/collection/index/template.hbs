<section class="content">
  <div class="row">
    <div class="col-md-12">
      <div class="box">
        <div class="box-header">
          <h3 class="box-title">{{model.name}}</h3>
          <div class="box-tools">
        <!-- <input type="text" class="form-control input-sm" placeholder="Search..."> -->
            <ul class="pagination pagination-sm no-margin pull-right">
              {{#if hasPrev}}
                <li><a {{action 'prevPage'}}>«</a></li>
              {{/if}}
              <li><a>{{page}}</a></li>
              {{#if hasNext}}
                <li><a {{action 'nextPage'}}>»</a></li>
              {{/if}}
            </ul>
          </div>
        </div>
        <div class="box-body">
          {{#if jsTreeData.isPending}}
            <div style="text-align: center;">
              <i class="fa fa-spinner fa-pulse"></i>
            </div>
          {{else}}
            {{#ember-fancytree data=jsTreeData.data}}
              <colgroup>
                <col width="40%">
                <col width="55%">
                <col width="5%">
              </colgroup>
              <thead>
                <tr>
                  <th>Key</th>
                  <th>Value</th>
                  <th>Type</th>
                </tr>
              </thead>
              <tbody>
              </tbody>
            {{/ember-fancytree}}
          {{/if}}
        </div>
      </div>
    </div>
  </div>
</section>

{{outlet}}
