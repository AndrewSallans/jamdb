{
  "data": {
    "meta": {
      "meta": {
        "create-on": {
          "format": "date-time",
          "type": "string"
        },
        "modified-on": {
          "format": "date-time",
          "type": "string"
        },
        "created-by": {
          "type": "string",
          "pattern": "^.+\\-.*\\-.+$"
        },
        "modified-by": {
          "type": "string",
          "pattern": "^.+\\-.*\\-.+$"
        }
      }
    },
    "required": [
      "attributes",
      "type"
    ],
    "properties": {
      "id": {
        "type": "string",
        "pattern": "[\\d\\w\\.\\-]{3,64}"
      },
      "type": {
        "type": "string"
      },
      "attributes": {
        "required": [
          "type",
          "properties"
        ],
        "type": "object",
        "properties": {
          "permissions": {
            "type": "object",
            "patternProperties": {
              "^\\w+\\-w*\\-\\w+$": {
                "enum": [
                  "READ",
                  "WRITE",
                  "CREATE",
                  "UPDATE",
                  "DELETE",
                  "ADMIN"
                ]
              }
            }
          }
        }
      }
    },
    "additionalProperties": false
  },
  "required": [
    "data"
  ],
  "additionalProperties": false
}